  "tokenColors": [
    {
      "name": "Comments - lighter for less distraction",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#B0A090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings - soft olive green",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#7A9A5A"
      }
    },
    {
      "name": "Numbers - muted gold",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#A8894A"
      }
    },
    {
      "name": "Constants - soft terracotta",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#B07868"
      }
    },
    {
      "name": "Keywords - warm brown, no bold for less strain",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9B6842"
      }
    },
    {
      "name": "Control Keywords",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#A8704A"
      }
    },
    {
      "name": "Operators - subtle",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#7D5A3C"
      }
    },
    {
      "name": "Functions - soft blue",
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#5A8AAA"
      }
    },
    {
      "name": "Classes - muted gold, no bold",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#A88A3A"
      }
    },
    {
      "name": "Types - soft purple",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#9A7ABB"
      }
    },
    {
      "name": "Variables - main text color",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#6B5344"
      }
    },
    {
      "name": "Parameters - slightly different, no italic for readability",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#7D5A3C"
      }
    },
    {
      "name": "Properties - soft teal",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#6A9A9A"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#9B6842"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A88A3A"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#5A8AAA"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#7A9A5A"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#A8894A"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#B07868"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#6A9A9A"
      }
    },
    {
      "name": "Punctuation - subtle",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#8A7060"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#9B6842",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#7D5A3C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#A8704A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#5A8AAA"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#7A9A5A"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#9B6842"
      }
    },
    {
      "name": "Invalid - softer red background",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#B86B5C"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#A8704A"
      }
    }
  ],
  "semanticHighlighting": true,
